{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "/usr/local/bin/g++",
      "args": [
        "-g",
        "-fopenmp",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "--std=c++17",
        "-Wno-psabi",
        "-march=native",
        
        // "-mavx512vl"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}